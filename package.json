{
  "name": "moleculer",
  "version": "0.7.1",
  "description": "Fast & powerful microservices framework for Node.JS",
  "main": "index.js",
  "scripts": {
    "demo": "node examples/index.js",
    "demow": "nodemon examples/index.js",
    "bench": "node benchmark/index.js",
    "perf": "nodemon --allow-natives-syntax benchmark/perf-runner.js",
    "pperf": "node --inspect --expose-gc benchmark/perf-runner.js",
    "ci": "jest --watch",
    "test": "jest --coverage",
    "lint": "eslint --ext=.js src test",
    "deps": "npm-check -u",
    "coverall": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "proto": "pbjs -t static-module -w commonjs -o src/serializers/proto/packets.proto.js src/serializers/proto/packets.proto",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "keywords": [
    "microservices",
    "microservice",
    "rpc",
    "services",
    "micro",
    "pubsub",
    "scalable"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ice-services/moleculer.git"
  },
  "author": "Ice Services",
  "license": "MIT",
  "devDependencies": {
    "benchmarkify": "2.1.0",
    "body-parser": "1.17.1",
    "coveralls": "2.13.1",
    "eslint": "3.19.0",
    "express": "4.15.2",
    "fakerator": "0.3.0",
    "jest": "19.0.2",
    "lolex": "1.6.0",
    "nodemon": "1.11.0",
    "npm-check": "5.4.0",
    "serve-static": "1.12.2",
    "socket.io": "1.7.3",
    "v8-natives": "1.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "rootDir": "./src",
    "roots": [
      "../test"
    ]
  },
  "dependencies": {
    "amqplib": "0.5.1",
    "avsc": "5.0.1",
    "bluebird": "3.5.0",
    "chalk": "1.1.3",
    "es6-error": "4.0.2",
    "eventemitter2": "4.1.0",
    "fastest-validator": "0.4.2",
    "glob": "7.1.1",
    "ioredis": "2.5.0",
    "lodash": "4.17.4",
    "mqtt": "2.7.1",
    "msgpack5": "3.4.1",
    "nanomatch": "1.1.1",
    "nats": "0.7.16",
    "node-object-hash": "1.2.0",
    "protobufjs": "6.7.3",
    "snyk": "^1.30.1"
  },
  "engines": {
    "node": ">= 6.x.x"
  },
  "tonicExampleFilename": "docs/runkit/simple.js",
  "typings": "./index.d.ts",
  "snyk": true
}
